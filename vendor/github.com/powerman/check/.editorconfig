# Doc: https://EditorConfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 94

[*.md]
indent_size = 2

[*.{yml,yaml}]
indent_size = 2

[{*.go,go.mod,go.work}]
indent_style = tab
indent_size = unset

[*.proto]
indent_size = 2
